<config>
   <acl>
    <resources>
        <all>
        <title>Allow Everything</title>
        </all>
        <admin>
            <children>
                <system>
                    <children>
                        <config>
                            <children>
                                <stylisthub translate="title" module="stylisthub">
                                    <title>Stylisthub Section</title>
                                </stylisthub>
                            </children>
                        </config>
                    </children>
                </system>
                <stylisthub module="stylisthub">
                <title>stylisthub</title>
                <children>
                    <managestylist module="stylisthub">
                        <title>Manage Stylists</title>
                    </managestylist>
                    <order module="stylisthub">
                        <title>Stylist Orders</title>
                    </order>
                    <commission module="stylisthub">
                        <title>Commission detail</title>
                    </commission>
                    <manage_products module="stylisthub">
                        <title>Stylist Products</title>
                    </manage_products>
                    <stylistreview module="stylisthub">
                        <title>Review and Ratings</title>
                    </stylistreview>
                    <configuration module="stylisthub">
                        <title>Configuration</title>
                    </configuration>
                </children>
            </stylisthub>
           </children>
       </admin>
   </resources>
</acl>
</config>
